<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension point="org.eclipse.ui.commands">
      <category
            name="Sample Category"
            id="com.samebug.clients.eclipse.commands.category">
      </category>
      <command
            name="Sample Command"
            categoryId="com.samebug.clients.eclipse.commands.category"
            id="com.samebug.clients.eclipse.commands.sampleCommand">
      </command>
      <command
            name="show api key"
            categoryId="com.samebug.clients.eclipse.commands.category"
            id="com.samebug.clients.eclipse.commands.showApiKey">
      </command>
      <command
            name="Samebug"
            categoryId="com.samebug.clients.eclipse.commands.category"
            id="com.samebug.clients.eclipse.commands.popupTest">
      </command>
   </extension>
   <extension point="org.eclipse.ui.handlers">
      <handler
            commandId="com.samebug.clients.eclipse.commands.sampleCommand"
            class="com.samebug.clients.eclipse.handlers.SampleHandler">
      </handler>
       <handler
            commandId="com.samebug.clients.eclipse.commands.popupTest"
            class="com.samebug.clients.eclipse.handlers.PopupHandler">
      </handler>
      <handler
            commandId="com.samebug.clients.eclipse.commands.showApiKey"
            class="com.samebug.clients.eclipse.handlers.ShowApiKey">
      </handler>
       <handler
            commandId="com.samebug.clients.eclipse.commands.popupTest"
            class="com.samebug.clients.eclipse.handlers.PopupHandler">
      </handler>
   </extension>
   <extension point="org.eclipse.ui.bindings">
      <key
            commandId="com.samebug.clients.eclipse.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="Sample Menu"
               mnemonic="M"
               id="com.samebug.clients.eclipse.menus.sampleMenu">
            <command
                  commandId="com.samebug.clients.eclipse.commands.sampleCommand"
                  mnemonic="S"
                  id="com.samebug.clients.eclipse.menus.sampleCommand">
            </command>
            <command
   				commandId="com.samebug.clients.eclipse.commands.showApiKey"
            		mnemonic="S"
            		id="com.samebug.clients.eclipse.menus.showApiKey">
      		</command>
      		  <command
                  commandId="com.samebug.clients.eclipse.commands.popupTest"
                  id="com.samebug.clients.eclipse.commands.popupTest">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <command
               commandId="com.samebug.clients.eclipse.commands.popupTest"
               label="test33"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="com.samebug.clients.eclipse.toolbars.sampleToolbar">
            <command
                  commandId="com.samebug.clients.eclipse.commands.sampleCommand"
                  icon="icons/samebug-15x15.png"
                  tooltip="Say hello world"
                  id="com.samebug.clients.eclipse.toolbars.sampleCommand">
            </command>
         </toolbar>
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <command
               commandId="com.samebug.clients.eclipse.commands.popupTest"
               label="test33"
               style="push">
         </command>
        </menuContribution>
   </extension>
   <extension point="org.eclipse.debug.ui.consoleLineTrackers">
   <consoleLineTracker 
     id="com.samebug.clients.eclipse.handlers.ConsoleLineTracker"
     class="com.samebug.clients.eclipse.handlers.ConsoleLineTracker"
     processType="java">
  </consoleLineTracker>
   </extension>
    <extension point="org.eclipse.ui.views">
 	 <category
            name="Sample Category"
            id="com.test">
      </category>
      <view
            name="Samebug"
            inject="true"
            icon="icons/samebug-15x15.png"
            category="com.test"
            class="com.samebug.clients.eclipse.views.BrowserView"
            id="com.samebug.clients.eclipse.views.BrowserView">
      </view>
    </extension>
    <extension
          point="org.eclipse.ui.perspectiveExtensions">
       <perspectiveExtension
             targetID="org.eclipse.jdt.ui.JavaPerspective">
          <view
                id="com.samebug.clients.eclipse.views.BrowserView"
                ratio="0.5"
                relationship="right"
                relative="org.eclipse.ui.views.ProblemView">
          </view>
       </perspectiveExtension>
    </extension>
    <extension
          point="org.eclipse.help.contexts">
       <contexts
             file="contexts.xml">
       </contexts>
    </extension>
    <extension point="org.eclipse.ui.preferencePages">
    			<page id="com.samebug.clients.eclipse.handlers.PreferencePage"
	    		class="com.samebug.clients.eclipse.handlers.PreferencePage"
	    		category="org.eclipse.ui.preferencePages.Workbench"
            name="Samebug Configuration">
       </page>
    </extension>
    <extension point="org.eclipse.core.runtime.preferences">
    </extension>
 

</plugin>
